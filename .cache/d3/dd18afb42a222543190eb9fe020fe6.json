\nimport { actions } from \"../../store/actions\";\n\nimport \"./styles.css\";\n\ninterface Props {\n    selection: Selection;\n    onSelect: (selection: Selection) => void;\n}\n\nexport const FlowList: FC<Props> = ({ selection, onSelect }) => {\n    const flows = useStore((s) => {\n        return s.score.flows.order.map((key) => {\n            return s.score.flows.by_key[key];\n        });\n    });\n    const width = `calc(${100 / flows.length}% - 8px)`;\n\n    return (\n        <div className=\"flow-list\">\n            <div className=\"flow-list__header\">\n                <span>Flows</span>\n                <Icon\n                    size={24}\n                    path={mdiPlus}\n                    onClick={() => {\n                        const key = actions.score.flow.create();\n                        onSelect({ key, type: SelectionType.Flow });\n                    }}\n                />\n            </div>\n            <div className=\"flow-list__wrapper\">\n                <SortableContainer\n                    direction=\"x\"\n                    className=\"flow-list__content\"\n                    onEnd={actions.score.flow.reorder}\n                >\n                    {flows.map((flow, i) => (\n                        <FlowItem\n                            index={i}\n                            key={flow.key}\n                            flow={flow}\n                            selection={selection}\n                            onSelect={onSelect}\n                            style={{ width }}\n                        />\n                    ))}\n                </SortableContainer>\n            </div>\n        </div>\n    );\n};\n"},"lineCount":null}},"error":null,"hash":"4f4e2f926600851d468d7ff5cef25da3","cacheData":{"env":{}}}