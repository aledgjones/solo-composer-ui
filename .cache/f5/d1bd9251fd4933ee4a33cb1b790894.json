, setSettings] = useState<boolean>(false);\n\n    return (\n        <>\n            <div className=\"player-list\">\n                <div className=\"player-list__header\">\n                    <span className=\"player-list__label\">Players</span>\n                    <Icon\n                        style={{ marginRight: 12 }}\n                        size={24}\n                        path={mdiCogOutline}\n                        onClick={() => setSettings(true)}\n                    />\n                    <Icon size={24} path={mdiPlus} onClick={onCreatePlayer} />\n                </div>\n                <SortableContainer\n                    direction=\"y\"\n                    className=\"player-list__content\"\n                    onEnd={actions.score.player.reorder}\n                >\n                    {players.map((player, i) => {\n                        return (\n                            <PlayerItem\n                                index={i}\n                                key={player.key}\n                                player={player}\n                                instruments={instruments}\n                                counts={counts}\n                                selected={\n                                    selection && player.key === selection.key\n                                }\n                                expanded={expanded[player.key]}\n                                onSelect={onSelect}\n                                onAddInstrument={onAddInstrument}\n                            />\n                        );\n                    })}\n                </SortableContainer>\n            </div>\n\n            <SetupSettings\n                width={900}\n                open={settings}\n                onClose={() => setSettings(false)}\n            />\n        </>\n    );\n};\n"},"lineCount":null}},"error":null,"hash":"0f14146109c27f56b56602f8a1540047","cacheData":{"env":{}}}